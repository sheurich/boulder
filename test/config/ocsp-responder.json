{
  "ocspResponder": {
    "db": {
      "dbConnectFile": "test/secrets/ocsp_responder_dburl",
      "maxOpenConns": 10
    },
    "path": "/",
    "listenAddress": "0.0.0.0:4002",
    "maxAge": "10s",
    "timeout": "4.9s",
    "shutdownStopTimeout": "10s",
    "debugAddr": ":8005",
